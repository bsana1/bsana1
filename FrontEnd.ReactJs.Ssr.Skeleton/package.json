{
  "name": "shopin3d-coordinator",
  "version": "1.0.0",
  "description": "Microsoft ShopIn3d - FE Server",
  "main": "si3d.coordinator.server.js",
  "scripts": {
    "build-client": "cross-env NODE_ENV=production webpack --config webpack.client.js --mode production",
    "build-server": "cross-env NODE_ENV=production webpack --config webpack.server.js --mode production",
    "build:azure:dev": "cross-env AZURE_ENV=dev npm run build-client  && cross-env AZURE_ENV=dev npm run build-server",
    "build:azure:ppe": "cross-env AZURE_ENV=ppe npm run build-client  && cross-env AZURE_ENV=ppe npm run build-server",
    "build:azure:prod": "cross-env AZURE_ENV=prod npm run build-client && cross-env AZURE_ENV=prod npm run build-server",
    "build": "npm run build:azure:dev && npm run build:azure:ppe && npm run build:azure:prod && npm run build:client:api",
    "server": "npm run build && npm run start",
    "lint": "eslint src --ext .js,.jsx,.ts.,tsx",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:ci": "npm run test:coverage -- --watchAll=false --reporters=default --reporters=jest-junit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-client:dev": "cross-env AZURE_ENV=localhost cross-env NODE_ENV=development webpack --config webpack.client.js --mode development --watch",
    "build-server:dev": "cross-env AZURE_ENV=localhost cross-env NODE_ENV=development webpack --config webpack.server.js --mode development",
    "build:dev": "npm run build-server:dev && npm build-client:dev ",
    "server:dev": "npm run build-server:dev && npm-run-all --parallel build-client:dev start:dev",
    "start:dev": "cross-env NODE_ENV=development node build/si3d-coordinator-server.js",
    "format": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "typecheck": "tsc -skipLibCheck",
    "start": "node build/si3d-coordinator-server.js",
    "start:ppe": "node build/si3d-coordinator-server.ppe.js",
    "start:prod": "node build/si3d-coordinator-server.prod.js",
    "build:client:api": "webpack --config webpack.api.js --mode production",
    "analyze:client:bundle": "cross-env ENABLE_BUNDLE_ANALYZER=true npm run build-client"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "@microsoft/1ds-analytics-web-js": "^3.1.6",
    "@microsoft/applicationinsights-web": "^2.6.5",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/redux-thunk": "^2.1.0",
    "applicationinsights": "^2.1.8",
    "axios": "^0.21.4",
    "babel-test": "^0.2.4",
    "body-parser": "^1.19.0",
    "classnames": "^2.3.1",
    "core-js": "^3.15.2",
    "css-loader": "^6.2.0",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "intersection-observer": "^0.12.0",
    "is-mobile": "^3.1.0",
    "jest": "^27.0.6",
    "mime": "^2.5.2",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "sprintf-js": "^1.1.2",
    "style-loader": "^3.2.1",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "uuid": "^8.3.2",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.4.13",
    "@types/react-router-config": "^5.0.3",
    "@types/redux-mock-store": "^1.0.3",
    "@types/sprintf-js": "^1.1.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.31.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "git-revision-webpack-plugin": "^5.0.0",
    "husky": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest-dom": "^4.0.0",
    "jest-junit": "^12.2.0",
    "mini-css-extract-plugin": "^2.1.0",
    "prettier": "2.3.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-mock-store": "^1.5.4",
    "supertest": "^6.2.3",
    "webpack": "^5.45.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "jest-junit": {
    "suiteNameTemplate": "{filepath}",
    "outputDirectory": ".",
    "outputName": "junit.xml"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run typecheck && npm run lint && npm run test",
      "pre-push": "npm run typecheck && npm run lint && npm run test"
    }
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "resolutions": {
    "shell-quote": "1.7.3"
  }
}
